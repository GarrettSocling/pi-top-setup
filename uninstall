#!/bin/bash
# Uninstall pi-top Configuration
sudo rm /usr/bin/pt-setup
sudo rm /usr/share/applications/pt_config.desktop
echo "menu->Preferences->pi-top Configuration has been uninstalled"

rm ~/bin/pt-log
echo "pt-log has been unistalled"

rm ~/bin/pt-devices
echo "pt-devices has been unistalled"

